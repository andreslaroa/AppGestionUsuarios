@{
    ViewData["Title"] = "Gestión de Usuarios";
}

<h2>Gestión de Usuarios</h2>


<form method="post" action="/UserManagement/CreateUser">
    <div>
        <label for="ouPrincipal">OU Principal:</label>
        <input type="text" id="ouPrincipal" name="ouPrincipal" required />
    </div>
    <div>
        <label for="ouSecundaria">OU Secundaria:</label>
        <input type="text" id="ouSecundaria" name="ouSecundaria" required />
    </div>
    <button type="submit">Enviar</button>
</form>
